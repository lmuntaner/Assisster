<div class="col-xs-5 active-step choose-time">
	<h4>Seleccionar Hora</h4>
	<h5><%= date.format('dddd, D MMMM') %></h5>
	<% if (availableSlots) { %> 
		<div class="list-group">
			<div id="loading" class="sk-spinner sk-spinner-three-bounce">
			  <div class="sk-bounce1"></div>
			  <div class="sk-bounce2"></div>
			  <div class="sk-bounce3"></div>
			</div>
		</div>
	<% } else { %>
		<div class="no-available-slots">
			<p>Lo siento, no hay horas libres</p>
		</div>
	<% } %>
</div>
<div class="col-xs-6 chosen-time">
</div>
