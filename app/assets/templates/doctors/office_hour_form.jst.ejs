<div class="my-modal">
</div>
<div class="modal-content appointment-content">
	<div class="panel-heading">
		<h3 class="panel-title">
			<% if (appointment.isNew()) { %>
				Crear Hora Oficina
			<% } else { %>
				Actualizar Hora Oficina
			<% } %>
		</h3>
	</div>
	<div class="panel-body">
		<form role="form">
			<div class="form-inputs">
				<a href="javascript:void(0)" id="open-more-days">Añadir más días</a>
				<div class="more-dates-pick">
				</div>
			</div>
			<button type="button"
							id="submit-office-hour-form"
							class="btn btn-primary">
							Guardar
			</button>
			<button type="button"
							id="close-office-hour-form"
							class="btn btn-default">
							Cerrar
			</button>
			<% if (!appointment.isNew() && appointment.get('appointment_status') !== 'Cancelled') { %>
				<button type="button"
								id="cancel-office-hour-form"
								class="btn btn-danger cancel">
								Cancelar
				</button>
			<% } %>
		</form>
	</div>
</div> <!-- /.modal-content -->