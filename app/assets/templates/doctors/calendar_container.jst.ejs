<div class="dashboard-title">
  <h2>Dashboard</h2>
</div>
<div class="dashboard-menu">
	<ul class="nav nav-tabs">
	  <li role="presentation"><a href="#">Home</a></li>
	  <li role="presentation" class="active"><a href="#calendar">Calendar</a></li>
		<li role="presentation"><a href="#appointments">Appointments</a></li>
	</ul>
</div>
<div class="dashboard-body">
	<div class="calendar-header">
		<div class='left-calendar-header'>
			<button type="button"
							class="btn btn-primary btn-sm create-appointment"
							data-toggle="tooltip"
							data-placement="top"
							title="You can click on the calendar as well">
							Create Appointment
			</button>
		</div>
		<div class='right-calendar-header'>
			<button type="button"
							class="btn btn-success btn-sm create-office-hours">
							Create Office Hours
			</button>
		</div>		
		<div class="center-calendar-header text-center">
			<button type="button"
							class="btn btn-default btn-xs zoom-out-calendar">
							-
			</button>
			<span>Zoom</span>
			<button type="button"
							class="btn btn-default btn-xs zoom-in-calendar">
							+
			</button>
		</div>

	</div>
</div>
