<div class="table-appointments-container" id="appointments">
	<div class="input-group">
	  <span class="input-group-addon">Search</span>
	  <input type="text" class="form-control search" placeholder="Username">
	</div>
	<table class="table appointments-table-header">
		<tr>
			<th class="table-fname">First Name</th>
			<th class="table-lname">Last Name</th>
			<th class="table-email">Email</th>
			<th class="table-title">Title</th>
			<th class="table-date">Appointment Date</th>
			<th class="table-time">Appointment Time</th>
			<th class="table-status">Status</th>
		</tr>
	</table>
	<table class="table">
		<tbody class="appointments list">
		</tbody>
	</table>
</div>